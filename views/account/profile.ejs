    <%- include('../partial/user-nav') %>


        <div class="p-6">

            <h1 class="md:text-5xl text-3xl font-extrabold text-transparent bg-clip-text bg-gradient-to-r to-emerald-600 from-sky-400"
                id="heading">
                <%= title %>
            </h1>
            <br>
            <div id="form" class="w-auto p-8">
                <blockquote class="text-xl italic font-semibold text-gray-900 dark:text-white">
                    <p>"Requies Two-Factor Authentication Disabled."</p>
                </blockquote>
                <br>
                <% if(msg!="" ){ %>
                    <div class="flex p-4 mb-4 text-sm text-green-800 rounded-lg bg-green-50 dark:bg-gray-800 dark:text-green-400"
                        role="alert">
                        <svg aria-hidden="true" class="flex-shrink-0 inline w-5 h-5 mr-3" fill="currentColor"
                            viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd"
